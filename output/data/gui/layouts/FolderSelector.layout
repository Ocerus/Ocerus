<?xml version="1.0" encoding="UTF-8"?>
<GUILayout>
	<Window Type="DefaultWindow" Name="FolderSelector">
		<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
		<Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}"/>
		<Window Type="Editor/FrameWindow" Name="FolderSelector/Frame">
			<Property Name="AlwaysOnTop" Value="True"/>
			<Property Name="Text" Value="$select_folder$"/>
			<Property Name="TitlebarFont" Value="DejaVuSans-10"/>
			<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
			<Property Name="TitlebarEnabled" Value="True"/>
			<Property Name="UnifiedAreaRect" Value="{{0.322461,0},{0.237659,0},{0.728711,0},{0.79682,0}}"/>
			<Property Name="SizingEnabled" Value="False"/>
			<Property Name="DragMovingEnabled" Value="False"/>
			<Property Name="CloseButtonEnabled" Value="False"/>
			<Window Type="Editor/Button" Name="FolderSelector/ButtonOK">
				<Property Name="Text" Value="$ok$"/>
				<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
				<Property Name="UnifiedAreaRect" Value="{{1,-64},{1,-28},{1,0},{1,0}}"/>
			</Window>
			<Window Type="Editor/Button" Name="FolderSelector/ButtonCancel">
				<Property Name="Font" Value="DejaVuSans-10"/>
				<Property Name="Text" Value="$cancel$"/>
				<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
				<Property Name="UnifiedAreaRect" Value="{{1,-132},{1,-28},{1,-68},{1,0}}"/>
			</Window>
			<Window Type="Editor/Editbox" Name="FolderSelector/PathBox">
				<Property Name="ReadOnly" Value="True"/>
				<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
				<Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,-40},{0,24}}"/>
			</Window>
			<Window Type="Editor/Button" Name="FolderSelector/ButtonCreateDirectory">
				<Property Name="Tooltip" Value="$create_directory$" />
				<Property Name="Font" Value="DejaVuSans-10"/>
				<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
				<Property Name="UnifiedAreaRect" Value="{{1,-40},{0,0},{1,0},{0,24}}"/>
				<Window Type="Editor/StaticImage" Name="FolderSelector/ButtonCreateDirectory/Image">
					<Property Name="UnifiedAreaRect" Value="{{0.5,-6},{0.5, -6},{0.5, 6},{0.5, 6}}"/>
					<Property Name="Image" Value="set:ResourceWindowIcons image:Directory" />
					<Property Name="MousePassThroughEnabled" Value="True" />
					<Property Name="FrameEnabled" Value="False" />
				</Window>
			</Window>
			<Window Type="Editor/Listbox" Name="FolderSelector/FolderList">
				<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
				<Property Name="UnifiedAreaRect" Value="{{0,0},{0,28},{1,0},{1,-60}}"/>
			</Window>
			<Window Type="Editor/StaticText" Name="FolderSelector/EditboxLabel">
				<Property Name="Text" Value="Name:"/>
				<Property Name="FrameEnabled" Value="False"/>
				<Property Name="UnifiedAreaRect" Value="{{0,0},{1,-56},{0,120},{1,-32}}"/>
				<Property Name="BackgroundEnabled" Value="False"/>
			</Window>
			<Window Type="Editor/Editbox" Name="FolderSelector/Editbox">
				<Property Name="UnifiedAreaRect" Value="{{0,120},{1,-56},{1,0},{1,-32}}"/>
				<Property Name="BackgroundEnabled" Value="False"/>
			</Window>
		</Window>
	</Window>
</GUILayout>
