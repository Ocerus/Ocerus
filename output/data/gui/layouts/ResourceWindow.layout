<?xml version="1.0" encoding="UTF-8"?>

<GUILayout >
    <Window Type="Editor/FrameWindow" Name="" >
        <Property Name="Font" Value="DejaVuSans-10" />
        <Property Name="Text" Value="$resources$" />
        <Property Name="TitlebarFont" Value="DejaVuSans-10" />
        <Property Name="SizingEnabled" Value="False" />
        <Property Name="TitlebarEnabled" Value="True" />
        <Property Name="UnifiedAreaRect" Value="{{0.85,0},{0.5,25},{1,0},{1,0}}" />
        <Property Name="DragMovingEnabled" Value="False" />
        <Property Name="CloseButtonEnabled" Value="False" />
		<Window Type="DefaultWindow" Name="/Toolbar">
			<Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{0,18}}" />
			<Window Type="Editor/ImageButton" Name="/Toolbar/Refresh">
				<Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{0,16},{1,0}}" />
                <Property Name="NormalImage" Value="set:EditorToolbar image:btnResumeActionNormal" />
                <Property Name="HoverImage" Value="set:EditorToolbar image:btnResumeActionHover"  />
                <Property Name="PushedImage" Value="set:EditorToolbar image:btnResumeActionPushed" />
                <Property Name="DisabledImage" Value="set:EditorToolbar image:btnResumeActionDisabled" />
                <Property Name="Tooltip" Value="$refresh_resources_hint$" />
			</Window>
		</Window>
		<Window Type="Editor/ItemListbox" Name="/List" >
            <Property Name="UnifiedAreaRect" Value="{{0,0},{0,20},{1,0},{1,0}}" />
        </Window>
   </Window>
</GUILayout>
