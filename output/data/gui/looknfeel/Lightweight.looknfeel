            <Area>
                <Dim type="LeftEdge" ><UnifiedDim scale="0.325" type="LeftEdge" /></Dim>
                <Dim type="TopEdge" >
                    <WidgetDim widget="__auto_decbtn__" dimension="Height">
                        <DimOperator op="Add">
