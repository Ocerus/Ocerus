            </Area>
            <VertAlignment type="BottomAligned" />
            <Property name="NormalImage" value="set:Lightweight image:VertScrollDownNormal" />
            <Property name="HoverImage" value="set:Lightweight image:VertScrollDownHover" />
            <Property name="PushedImage" value="set:Lightweight image:VertScrollDownNormal" />
